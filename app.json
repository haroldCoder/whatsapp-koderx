{
  "expo": {
    "name": "whatsapp-koderx",
    "slug": "whatsapp-koderx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "@/assets/whatsappdark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/wpp.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wpp.png",
        "backgroundColor": "#000"
      },
      "package": "com.koderx.myapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "87cd887e-b465-469c-acf7-60706ede50ac"
      }
    },
    "owner": "koderx"
  }
}
